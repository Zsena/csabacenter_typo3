<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Function</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.teaserwall.style>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.hometeaser.style</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.hometeaser.style.light</numIndex>
                                        <numIndex index="1">light</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.hometeaser.style.dark</numIndex>
                                        <numIndex index="1">dark</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.hometeaser.style.gray</numIndex>
                                        <numIndex index="1">gray</numIndex>
                                    </numIndex>
                                </items>
                                <size>1</size>
                                <defaultValue>light</defaultValue>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </settings.teaserwall.style>
                    <settings.teaserwall.types>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.teaserwall.types</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:tx_center_domain_model_records_event</numIndex>
                                        <numIndex index="1">tx_center_domain_model_records_event</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:tx_center_domain_model_records_job</numIndex>
                                        <numIndex index="1">tx_center_domain_model_records_job</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:tx_center_domain_model_records_service</numIndex>
                                        <numIndex index="1">tx_center_domain_model_records_service</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:tx_center_domain_model_records_offer</numIndex>
                                        <numIndex index="1">tx_center_domain_model_records_offer</numIndex>
                                    </numIndex>
                                    <numIndex index="7" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:tx_center_domain_model_records_coupon</numIndex>
                                        <numIndex index="1">tx_center_domain_model_records_coupon</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:tx_center_domain_model_records_news</numIndex>
                                        <numIndex index="1">tx_center_domain_model_records_news</numIndex>
                                    </numIndex>
                                    <numIndex index="5" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:tx_center_domain_model_records_blog</numIndex>
                                        <numIndex index="1">135</numIndex>
                                    </numIndex>
                                    <numIndex index="6" type="array">
                                        <numIndex index="0">LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:tx_center_domain_model_records_contentteaser</numIndex>
                                        <numIndex index="1">tx_center_domain_model_records_contentteaser</numIndex>
                                    </numIndex>
                                </items>
                                <size>10</size>
                                <minitems>0</minitems>
                                <maxitems>99</maxitems>
                            </config>
                        </TCEforms>
                    </settings.teaserwall.types>
                    <settings.teaserwall.buttonText>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.hometeaser.buttonText</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <max>255</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.teaserwall.buttonText>
                    <settings.teaserwall.buttonLink>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.hometeaser.buttonLink</label>
                            <config>
                                <type>input</type>
                                <renderType>inputLink</renderType>
                                <fieldControl>
                                    <linkPopup>
                                        <options>
                                            <blindLinkFields>class, params, title</blindLinkFields>
                                            <blindLinkOptions>mail, url, spec, file, folder</blindLinkOptions>
                                        </options>
                                    </linkPopup>
                                </fieldControl>
                                <eval>required,trim</eval>
                            </config>
                        </TCEforms>
                    </settings.teaserwall.buttonLink>
                    <settings.teaserwall.headline>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.hometeaser.headline</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <max>255</max>
                                <eval>required,trim</eval>
                            </config>
                        </TCEforms>
                    </settings.teaserwall.headline>
                    <settings.teaserwall.backgroundImage>
                        <TCEforms>
                            <label>LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.hometeaser.backgroundImage</label>
                            <config>
                                <type>inline</type>
                                <maxitems>99</maxitems>
                                <foreign_table>sys_file_reference</foreign_table>
                                <!--<foreign_field>uid_foreign</foreign_field>-->
                                <foreign_table_field>tablenames</foreign_table_field>
                                <foreign_label>uid_local</foreign_label>
                                <foreign_sortby>sorting_foreign</foreign_sortby>
                                <foreign_selector>uid_local</foreign_selector>
                                <foreign_selector_fieldTcaOverride type="array">
                                    <config>
                                        <appearance>
                                            <elementBrowserType>file</elementBrowserType>
                                            <elementBrowserAllowed>gif,jpg,jpeg,png,svg</elementBrowserAllowed>
                                        </appearance>
                                    </config>
                                </foreign_selector_fieldTcaOverride>
                                <foreign_match_fields type="array">
                                    <fieldname>image</fieldname>
                                </foreign_match_fields>
                                <appearance type="array">
                                    <newRecordLinkAddTitle>1</newRecordLinkAddTitle>
                                    <headerThumbnail>
                                        <field>uid_local</field>
                                        <height>64</height>
                                        <width>64</width>
                                    </headerThumbnail>
                                </appearance>
                            </config>
                        </TCEforms>
                    </settings.teaserwall.backgroundImage>
                    <settings.teaserwall.interestsOnly>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.teaserwall.interestsOnly</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.teaserwall.interestsOnly>
                    <settings.teaserwall.cols>
                        <TCEforms>
                            <label>LLL:EXT:center/Resources/Private/Language/locallang_db.xlf:flexform.teaserwall.numberInterests</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <max>255</max>
                                <eval>required,trim</eval>
                            </config>
                        </TCEforms>
                    </settings.teaserwall.cols>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>